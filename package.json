    "lerna-changelog": "^0.7.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "changelog": {
    "repo": "elwayman02/ember-data-github",
    "labels": {
      "breaking": "Breaking Change",
      "enhancement": "Enhancement",
      "bug": "Bug Fix",
      "documentation": "Documentation",
      "internal": "Internal"
    },
    "ignoreCommitters": [
      "greenkeeper[bot]"
