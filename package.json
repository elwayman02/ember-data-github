{
  "name": "ember-data-github",
  "version": "0.8.0",
  "description": "Ember Data abstraction for the GitHub API",
  "keywords": [
    "ember-addon",
    "ember-data",
    "github",
    "api"
  ],
  "license": "MIT",
  "author": "Jordan Hawker <hawker.jordan@gmail.com> (http://www.JordanHawker.com)",
